{
  "name": "legaycafe",
  "version": "1.0.0",
  "description": "Typescript, Socket.io, Graphql chat app for my roomates and I",
  "main": "dist/server/server.js",
  "scripts": {
    "postinstall": "npm run build",
    "heroku-postbuild": "npm run build",
    "test": "jest",
    "test:reset": "jest --updateSnapshot",
    "client": "parcel src/client/public/index.html",
    "start": "node 'dist/server/server.js'",
    "build": "npm run clean; npm run build:server; npm run build:client;",
    "start:dev": "npm run build:dev",
    "build:client": "parcel build src/client/public/index.html --out-dir dist",
    "build:server": "tsc -p . && ncp src/server/schema dist/server/schema",
    "build:dev": "nodemon 'src/server/server.ts' --exec 'ts-node' src/server/server.ts -e ts,graphql",
    "clear": "rm -rf node_modules; npm install",
    "clean": "rm -rf .cache; rm -rf dist;",
    "reset": "npm run clear; npm run clean; npm run build",
    "logs": "heroku logs --tail"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.8.0"
  },
  "author": "Yaniv Silberman",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/graphql-depth-limit": "^1.1.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "0.0.4",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "parcel-bundler": "^1.12.3",
    "react-test-renderer": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-graphql-loader": "^1.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.5",
    "@types/react-test-renderer": "^16.8.1",
    "@types/uuid": "^3.4.4",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-link-schema": "^1.2.2",
    "apollo-link-ws": "^1.0.17",
    "apollo-server": "^2.6.1",
    "apollo-server-express": "^2.6.1",
    "aws-sdk": "^2.471.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "draft-js": "^0.10.5",
    "draft-js-emoji-plugin": "^2.1.1",
